
//-----------------------------------------------------------------------------
myNth(POS, ARRAY, OUT) :- index(ARRAY, POS, 0, OUT).
//-----------------------------------------------------------------------------
index([], _, _, _) :- .fail.
//-----------------------------------------------------------------------------
index([H|R], IDX, IDX, H).
//-----------------------------------------------------------------------------
index([H|R], IDX, POS, OUT) :- index(R, IDX, POS+1, OUT).
//-----------------------------------------------------------------------------
