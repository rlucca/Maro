@article{nareyek2001review,
	Annote = {Este trabalho tenta delinear que a IA e os jogos modernos tem caracteristicas comuns:
(i) tempo real; (ii) ambientes dinamicos; (iii) conhecimento incompleto;
(iv) recursos restritos. Assim, tecnicas dos campos da IA como agentes autonomos,
planejamento, agendamento, robotica e aprendizado podem ser aplicadas para uma
variedade de tarefas em jogos modernos. Os personagens dos jogos podem ser vistos
como agentes da IA. Depois de uma breve introdu{\c c}{\~a}o a alguns dos principais
topicos estudados na IA (agentes autonomos, sistemas multiagentes e solucionador
de problema distribuido) s{\~a}o feitas breve descri{\c c}{\~o}es quanto aos tipos de agentes:
(i) reativo, orientados a regras (dificil de manter, regras tender a aumentar muito e
precisam ser exaustivas)(possuem uma rea{\c c}{\~a}o muito rapida); (ii) "triggering", podem
reagir t{\~a}o rapido quanto o reativo e ainda ter metas longas (entretanto, ainda esta
ligado a um conjunto de regras e n{\~a}o pode reagir apropriadamente a situa{\c c}{\~o}es nao
pensadas ou aprendidas a priori); (iii) deliberativos, diferem das abordagens anteriores
baseando-se em planos montados por um planejador com um estado do mundo atual
e descri{\c c}{\~a}o parcial das metas; (iv) hibridos, aplica um planejamento off-line para um
planejamento de alto nivel e deixa decis{\~o}es menores para um conjunto de regras;
(v) "anytime", agente constroi o plano de maneira interativa no tempo de computa{\c c}{\~a}o
que possui (tempos curtos de intera{\c c}{\~a}o deixo o plano reativo, a constru{\c c}{\~a}o interativa
permite uma grande adaptacao a mudan{\c c}a e eventos inesperados). Outros
requimentos s{\~a}o do planejador que deve considerar o planejamento temporal,
recursos, conhecimento incompleto e dominio especifico.},
	Author = {Nareyek, A.},
	Date-Added = {2011-01-19 17:14:30 -0200},
	Date-Modified = {2011-01-19 17:16:30 -0200},
	Journal = {Computers and Games},
	Pages = {414--422},
	Publisher = {Springer},
	Title = {{Review: Intelligent agents for computer games}},
	Year = {2001}}

@conference{damiano2008emotions,
	Annote = {Cada vez mais o paradigma dominante {\'e} personagens como agentes e um modelo
de emo{\c c}{\~o}es no personagem {\'e} necessario para aumentar o realismo e credibilidade
da historia sendo contada. Esse artigo se foca em dar um exemplo de como as
emo{\c c}{\~o}es aumentam o realismo na interpreta{\c c}{\~a}o dramaturga. O artigo explica uma
ontologia de dramaturgia para controlar os personagens no nivel mais abstrato
da cena e no nivel mais do personagem. Mas, sem entrar em detalhes sobre as
emo{\c c}{\~o}es. O modelo utilizado aqui foi o modelo OCC. E o exemplo {\'e} focado em
uma cena de Hamlet (Shakespeare), logo apos a cena do (to be or not to be), onde
hamlet tenta convercer Ophelia a abandonar a corte.},
