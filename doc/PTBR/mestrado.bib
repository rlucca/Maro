@article{ingrand1992architecture,
	Annote = {O trabalho apresenta os sistemas de raciocinadores situados que possuem a capacidade
de receber informa{\c c}{\~o}es de diversas fontes (sensores) e descobrir, quando possivel, onde
uma falha esta localizada. Al{\'e}m disso, ao ocorrer uma falha o sistema toma atitudes de
retifica{\c c}{\~a}o e de conten{\c c}{\~a}o da mesma. Dessa forma {\'e} importante saber quais tarefas tem
prioridades e o que pode ser colocado em paralelo. Assim, eh vital uma gerencia das
tarefas que se quer tomar e outro problema {\'e} fazer as a{\c c}{\~o}es no tempo correto rapidamente.
Algumas das caracteristicas desses sistemas s{\~a}o: assincronismo de eventos; garantir
rea{\c c}{\~a}o e tempo de resposta; representa{\c c}{\~a}o do conhecimento procedural (e n{\~a}o regras);
lidar com multiplos problemas (relacionados ou nao); comportamento reativo e dirigido a
meta; foco em aten{\c c}{\~a}o; reflexo (refletir o ambiente de forma internamente e repriorizar
tarefas {\'e} uma necessidade); opera{\c c}{\~a}o continua; lidar com dados incorretos/incompletos;
lidar com erros temporarios; modelar retorno demorado (valvula esta aberta, pediu para
fechar, por 10s a valvula ficara como aberta e, apos, estara como fechada); controle via
operador (um ser humano deve ser capaz de cooperar junto). A arquitetura para realizar
tudo isso {\'e} chamada de PRS (Procedural Reasoning System) e consiste de 5 coisas:
(1) um banco de dados das cren{\c c}as atuais do mundo; (2) um conjunto de metas;
(3) biblioteca de planos (ou metas) chamada de Conhecimento de Area;
(4) uma estrutura de inten{\c c}{\~a}o com os planos parcialmente ordenados para execu{\c c}{\~a}o;
(5) um mecanismo de inferencia que seleciona um plano na inten{\c c}{\~a}o baseado nas
metas e cren{\c c}as atuais para execu{\c c}{\~a}o.},
	Author = {Ingrand, F.F. and Georgeff, M.P. and Rao, A.S.},
	Date-Added = {2011-01-27 09:31:20 -0200},
	Date-Modified = {2011-01-27 10:02:53 -0200},
	Journal = {IEEE EXPERT INTELLIGENT SYSTEMS and THEIR APPLICATIONS},
	Pages = {34-44},
	Publisher = {Published by the IEEE Computer Society},
	Title = {{An architecture for real-time reasoning and system control}},
	Year = {1992}}

@article{shoham1993agent,
	Annote = {Esse trabalho apresenta um novo paradigma de programa{\c c}{\~a}o chamado Programa{\c c}{\~a}o de Agentes (AOP) promovendo uma vis{\~a}o social da computa{\c c}{\~a}o em que multiplos agentes interagem uns com os outros. Algumas propriedades de agentes {\'e} que eles s{\~a}o autonomos, de opera{\c c}{\~a}o continua e de "alto n{\'\i}vel" (representa{\c c}{\~a}o simbolica e/ou algumas fun{\c c}{\~o}es cogntivas-liked). O autor defende que agentes devem ser vistos por consistirem de componentes mentais como cren{\c c}as, capacidades, escolhas (decis{\~o}es) e comprometimentos. O autor explica esses componentes em demasia e tra{\c c}a uma diferen{\c c}a entre AOP e OOP. Alem de implementar um exemplo para o interpretador da linguagem sendo proposta (AGENT-0).},
	Author = {Shoham, Y.},
