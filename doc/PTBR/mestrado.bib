@conference{ben2010social,
	Annote = {Esse trabalho estua a evolu{\c c}{\~a}o das redes atrav{\'e}s de experimentos, conduzidos
ao longo de 8 semestres com 602 estudantes de MBA, usando simula{\c c}{\~a}o. O uso
da simula{\c c}{\~a}o visa: (i) estabelecer um ambiente realistico para os testes das redes
sociais pela cria{\c c}{\~a}o de redes simuladas; (ii) ganhar melhor conhecimento sobre
as estruturas das companhias, performance e condu{\c c}{\~a}o. Nenhum estudo at{\'e} agora
usou redes para criar redes, onde seriam extra{\'\i}das medidas. O presente trabalho
apresenta a trajetoria centralizada que {\'e} baseada no grau (numero de liga{\c c}{\~o}es
que um elemento da rede tem) e a redundancia da rede (um indice que mede a
existencia de buracos estruturais). A tomada de decis{\~a}o na simula{\c c}{\~a}o {\'e} feita pela
analise da empresa (nodo da rede), intera{\c c}{\~a}o com as outras companhias e restri{\c c}{\~o}es
que o jogador possui. A medida de trajetoria centralizada apresenta 4 tipos diferentes:
(a) baixa energia, estritamente abaixo da media de colabora{\c c}{\~a}o; (b) aumento de
energia, um aumento na colabora{\c c}{\~a}o continuo sobre o tempo; (c) alta energia,
estritamente acima da media de colabora{\c c}{\~a}o; (d) declinio de energia, um continuo
decrescimo na colabora{\c c}{\~a}o sobre o tempo. A colabora{\c c}{\~a}o {\'e} considerada como o
grau. Basicamente, o grau de cada nodo {\'e} comparado contra o grau medio do grafo.
No artigo todos os resultados foram utilizados para montar uma regress{\~a}o e com a
finalidade de comparar a categoriza{\c c}{\~a}o da nova medida com esta. A conclus{\~a}o {\'e} que
empresas com alta energia s{\~a}o sustentaveis e, normalmente, s{\~a}o de sucesso,
enquanto, que empresas com baixa energia por terem a tendencia ao isolamento
tendem a falhar.
},
	Author = {Ben-Zvi, T.},
	Booktitle = {Proceedings of the Behavioral and Quantitative Game Theory: Conference on Future Directions},
	Date-Added = {2011-01-16 09:20:11 -0200},
	Date-Modified = {2011-01-16 09:21:12 -0200},
	Organization = {ACM},
	Pages = {1--4},
	Title = {{Social networks analysis: a game experiment}},
	Year = {2010}}

@conference{lebowitz1983creating,
	Annote = {O artigo descreve uma parte do desenvolvimento de um programa chamado
