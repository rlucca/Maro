@article{dias2005feeling,
	Abstract = {O trabalho utiliza o sistema FearNot que foi desenvolvido para reduzir os problemas de bullying nas escolas. As crian{\c c}as s{\~a}o expostas a cenas de bullying e fazem o papel do amigo imaginario da vitima que pode sugerir formas de a{\c c}{\~a}o para situa{\c c}{\~o}es parecidas. A arquitetura criada foi pensanda tendo em mente: ter capacidades reativas e cognitivas, gerar credibilidade e empatia, possuir intera{\c c}{\~a}o com o usu{\'a}rio (note que os agentes n{\~a}o seguem cegamnte o que o usuario diz porque isso baixaria a credibilidade) e ser independente de dominio.

O autor cita Thomas and Johnston (Wall Disney) que falam que revelar os sentimentos dos personagens tamb{\'e}m revela sua personalidade. Eles tamb{\'e}m definem tr{\^e}s requisitos para o sucesso da express{\~a}o emocional: deve ser percebido sem nenhuma d{\'u}vida pela plateia; emo{\c c}{\~o}es podem ser acentuadas ou exageradas para um melhor entendimento;  emo{\c c}{\~o}es afeitam o processo de delibera{\c c}{\~a}o e consequencias devem ser notadas nas a{\c c}{\~o}es dos personagens.

O processo de delibera{\c c}{\~a}o do agente {\'e} focado na avalia{\c c}{\~a}o feita a partir do que se percebe do ambiente em niveis reativos e deliberativos (emo{\c c}{\~o}es com probabilidade). Apos o estado emocional {\'e} gerado e a memoria atualizada paralelamente. O processo de imita{\c c}{\~a}o pode ser tamb{\'e}m no nivel reativo (tendencias de a{\c c}{\~o}es) ou deliverativo (foco no problema ou emo{\c c}{\~a}o). Quando o nivel deliverativo procede, uma reavalia{\c c}{\~a}o pode ser feita que afeta o estado emocional (NOTA: caminho do cortex?) ou o entendimento do conhecimento (memorias: modelo do mundo, metas, intencoes e planos).

},
	Author = {Dias, J. and Paiva, A.},
	Date-Added = {2011-10-24 11:27:49 -0200},
	Date-Modified = {2011-10-24 15:25:21 -0200},
	Journal = {Progress in artificial intelligence},
	Pages = {127-140},
	Publisher = {Springer},
	Title = {Feeling and reasoning: A computational model for emotional characters},
	Year = {2005}}

@article{gratch2004domain,
	Abstract = {O foco do trabalho {\'e} a integra{\c c}{\~a}o de processos de raciocinadores simbolicos como planejar, agir, comuni{\c c}{\~a}o em linguagem natural e modelagem do usu{\'a}rio em um {\'u}nico sistema e o papel do processamento da emo{\c c}{\~a}o serve para tudo isso. O processo criado foi baseado em dois processos simples: avalia{\c c}{\~a}o (relacionado com o ambiente) e imita{\c c}{\~a}o (que sugere estrategias de a{\c c}{\~o}es a serem tomadas). As avalia{\c c}{\~o}es s{\~a}o baseadas em `variaveis de avalia{\c c}{\~a}o' (componentes de avalia{\c c}{\~a}o ou dimens{\~o}es de avalia{\c c}{\~a}o s{\~a}o outros nomes). O autor identificou as que mais aparecem nos trabalhos e se baseou nessas para criar seu sistema chamado EMA (EMotion and Adaptation).

Al{\'e}m disso, as estrategias de imita{\c c}{\~a}o podem ser baseadas no problema ou emocional. As baseadas no problema, normalmente, possuem alguma a{\c c}{\~a}o envolvida e foram encontradas 3 estrategias (copia ativa [tenta fazer], procurar suporte social [procurar informa{\c c}{\~a}o, conselheiro, etc] e planejar). O foco emocional tem ao todo 11 estrategias encontradas, elas se baseam em muitas vezes rever as prioridades anteriormente classificadas para melhorar o entendimento do mundo. Por exemplo, {\'e} possivel aceitar uma falha, negar o que esta acontecendo, procurar uma interpreta{\c c}{\~a}o positiva ou, at{\'e} mesmo, fazer uma outra atividade para evitar fazer o que {\'e} necessario (mental disengagement. Nota: No portugues isso seria procastinar! hehehehe).  Cada uma dessas estrategias s{\~a}o bem explicadas juntamente com exemplo de como o processo de imita{\c c}{\~a}o acontece em 5 estagios (identifica{\c c}{\~a}o de oportunidade, elabora{\c c}{\~a}o da situa{\c c}{\~a}o, proposi{\c c}{\~a}o de alternativas, estimativa do potencial de imita{\c c}{\~a}o e sele{\c c}{\~a}o da estrategia). O trabalho se baseia no trabalho de Elliot (1992 - affective reasoner) que {\'e} baseado no OCC. Uma das coisas interessantes aqui {\'e} que s{\~a}o usados conceitos de frames quando esta sendo feita a avalia{\c c}{\~a}o, al{\'e}m disso o agente mantem uma rela{\c c}{\~a}o do que aconteceu no passado, o que esta acotecendo e do futuro. O mecanismo de avalia{\c c}{\~a}o permite criar avalia{\c c}{\~o}es contraditorias ou com multiplos entendimentos do mundo, entretanto para o mecanismo de cria{\c c}{\~a}o de avalia{\c c}{\~a}o n{\~a}o sair (enloquecido) criando avalia{\c c}{\~o}es foi usado um threshold que quando atingindo {\'e} considerado um evento importante e que merece ser avaliado, similar {\`a} id{\'e}ia de ter sua aten{\c c}{\~a}o focada.

Eu acredito que esse trabalho eh uma estens{\~a}o do que foi iniciado em 2000 pelo Gratch.},
	Author = {Jonathan Gratch and Stacy Marsella},
	Date-Added = {2011-10-24 11:24:43 -0200},
	Date-Modified = {2011-10-24 15:06:20 -0200},
	Journal = {Cognitive Systems Research},
	Number = {4},
	Pages = {269-306},
	Publisher = {Elsevier},
	Title = {A domain-independent framework for modeling emotion},
	Volume = {5},
	Year = {2004}}

