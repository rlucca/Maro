<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
]>


<rdf:RDF xmlns="http://www.affective.org/occ_2011.owl#"
     xml:base="http://www.affective.org/occ_2011.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://www.affective.org/occ_2011.owl"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.affective.org/occ_2011.owl#hasCauseBy -->

    <owl:ObjectProperty rdf:about="http://www.affective.org/occ_2011.owl#hasCauseBy">
        <rdfs:range rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Situation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasEmotionType -->

    <owl:ObjectProperty rdf:about="http://www.affective.org/occ_2011.owl#hasEmotionType">
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Emotion"/>
        <rdfs:range rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasLikeTarget -->

    <owl:ObjectProperty rdf:about="http://www.affective.org/occ_2011.owl#hasLikeTarget">
        <rdfs:comment xml:lang="pt">Ao se gostar, se gosta de outros agentes.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#like -->

    <owl:ObjectProperty rdf:about="http://www.affective.org/occ_2011.owl#like">
        <rdfs:comment xml:lang="pt">Um agente gosta de outro agente com um valor.
(Por isso ser uma relacao sempre binaria tem que ter uma classe no meio)</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.affective.org/occ_2011.owl#hasDesire -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasDesire">
        <rdfs:comment xml:lang="pt">Desejo para o proprio agente.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Event"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasDesireForOther -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasDesireForOther">
        <rdfs:comment xml:lang="pt">Desejo presumido para outro agente pelo proprio agente.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Event"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasEffort -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasEffort">
        <rdfs:comment xml:lang="pt">Esse eh o esforco para tentar atrair ou repelir um determinado evento provavel.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Event"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasEmotionValence -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasEmotionValence">
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Emotion"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasFamiliarity -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasFamiliarity">
        <rdfs:comment xml:lang="pt">Quanto mais um objeto eh familiar mais ele eh atrativo, quanto menos mais ele eh repulsivo</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Object"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasJudge -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasJudge">
        <rdfs:comment xml:lang="pt">Como a acao feita esta sendo julgada.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Action"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasLikeLevel -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasLikeLevel">
        <rdfs:comment xml:lang="pt">Nivel de quanto se gosta.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasName -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="pt">Toda entidade deliberadora deve ter um nome, se nao como vamos nos referir a ela?</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.affective.org/occ_2011.owl#hasProbability -->

    <owl:DatatypeProperty rdf:about="http://www.affective.org/occ_2011.owl#hasProbability">
        <rdfs:comment xml:lang="pt">Um evento pode ter uma probabilidade de ocorrer.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.affective.org/occ_2011.owl#Event"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.affective.org/occ_2011.owl#Action -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Action">
        <rdfs:subClassOf rdf:resource="http://www.affective.org/occ_2011.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasJudge"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasJudge"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Event"/>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Object"/>
        <rdfs:comment xml:lang="pt">Acoes sao situacoes onde o responsavel eh avaliado.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Agent -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Agent">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasName"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasName"/>
                <owl:allValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#like"/>
                <owl:allValuesFrom rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Appraisal"/>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Emotion"/>
        <rdfs:comment xml:lang="pt">Entidade que pode ser causadora de eventos/acoes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Appraisal -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Appraisal">
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Emotion"/>
        <rdfs:comment xml:lang="pt">Formas de avaliacoes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Emotion -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Emotion">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEmotionType"/>
                <owl:allValuesFrom rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEmotionType"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEmotionValence"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEmotionValence"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">Emocao sendo presenciada (e nao percebida).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#EmotionType -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#EmotionType">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Gratitude"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Distress"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#FearsConfirmed"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#HappyFor"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Hate"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Relief"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Reproach"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Hope"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Gloating"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Joy"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Disappointment"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Resentment"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Love"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Pride"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Shame"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Admiration"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Anger"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Gratification"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#FearsUnconfirmed"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#SorryFor"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Satisfaction"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Remorse"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="pt">Emocoes possiveis propostas no modelo.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Event -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Event">
        <rdfs:subClassOf rdf:resource="http://www.affective.org/occ_2011.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasDesireForOther"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasDesireForOther"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasProbability"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasProbability"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasDesire"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEffort"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasEffort"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasDesire"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#LikeRelation"/>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Object"/>
        <rdfs:comment xml:lang="pt">Eventos sao situacoes onde as consequencias sao avaliadas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#LikeRelation -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#LikeRelation">
        <rdfs:subClassOf rdf:resource="http://www.affective.org/occ_2011.owl#Appraisal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasLikeLevel"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasLikeTarget"/>
                <owl:allValuesFrom rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasLikeTarget"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasLikeLevel"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.affective.org/occ_2011.owl#Object"/>
        <rdfs:comment xml:lang="pt">Relacao entre um agente alvo e um valor de agradabilidade.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Object -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Object">
        <rdfs:subClassOf rdf:resource="http://www.affective.org/occ_2011.owl#Appraisal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasFamiliarity"/>
                <owl:allValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasFamiliarity"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">Forma de percepcao quanto atratividade as coisas, objetos ou, ate mesmo, outros agentes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.affective.org/occ_2011.owl#Situation -->

    <owl:Class rdf:about="http://www.affective.org/occ_2011.owl#Situation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Action"/>
                    <rdf:Description rdf:about="http://www.affective.org/occ_2011.owl#Event"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasCauseBy"/>
                        <owl:allValuesFrom rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.affective.org/occ_2011.owl#hasCauseBy"/>
                        <owl:onClass rdf:resource="http://www.affective.org/occ_2011.owl#Agent"/>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.affective.org/occ_2011.owl#Appraisal"/>
        <rdfs:comment xml:lang="pt">Evento ou Acao.</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.affective.org/occ_2011.owl#Admiration -->

    <owl:NamedIndividual rdf:about="http://www.affective.org/occ_2011.owl#Admiration">
        <rdf:type rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:NamedIndividual>
    


    <!-- http://www.affective.org/occ_2011.owl#Anger -->

    <owl:NamedIndividual rdf:about="http://www.affective.org/occ_2011.owl#Anger">
        <rdf:type rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:NamedIndividual>
    


    <!-- http://www.affective.org/occ_2011.owl#Disappointment -->

    <owl:NamedIndividual rdf:about="http://www.affective.org/occ_2011.owl#Disappointment">
        <rdf:type rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:NamedIndividual>
    


    <!-- http://www.affective.org/occ_2011.owl#Distress -->

    <owl:NamedIndividual rdf:about="http://www.affective.org/occ_2011.owl#Distress">
        <rdf:type rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:NamedIndividual>
    


    <!-- http://www.affective.org/occ_2011.owl#FearsConfirmed -->

    <owl:NamedIndividual rdf:about="http://www.affective.org/occ_2011.owl#FearsConfirmed">
        <rdf:type rdf:resource="http://www.affective.org/occ_2011.owl#EmotionType"/>
    </owl:NamedIndividual>
    


    <!-- http://www.affective.org/occ_2011.owl#FearsUnconfirmed -->

